<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    to create record in subject-->
    <record id="school_subject_demo_english" model="school.subject">
        <field name="name">English</field>
        <field name="subject_department_id" eval="[Command.link(ref('school_department_demo_up')),Command.link(ref('school_department_demo_lp'))]"/>
    </record>

    <record id="school_subject_demo_maths" model="school.subject">
        <field name="name">Maths</field>
        <field name="subject_department_id" eval="[Command.link(ref('school_department_demo_up')),Command.link(ref('school_department_demo_hs'))]"/>
    </record>
</odoo>